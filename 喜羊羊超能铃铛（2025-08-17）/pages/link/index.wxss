.popup-mask {
    background: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 100;
}

.popup-mask .popup-container {
    background-color: #fff;
    border-radius: 40rpx 40rpx 0rpx 0rpx;
    bottom: 0;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    flex-direction: column;
    left: 0;
    overflow: hidden;
    padding-bottom: env(safe-area-inset-bottom);
    position: absolute;
    width: 100%;
    z-index: 101;
}

.popup-mask .popup-container .popup-header {
    box-sizing: border-box;
    height: 88rpx;
    padding: 24rpx 40rpx;
    text-align: right;
}

.popup-mask .popup-container .popup-header image {
    height: 40rpx;
    width: 40rpx;
}

.popup-mask .popup-container .popup-body {
    height: 524rpx;
    position: relative;
    text-align: center;
}

.popup-mask .popup-container .popup-body > image {
    height: 376rpx;
    width: 670rpx;
}

.popup-mask .popup-container .popup-body .tips {
    color: #000;
    font-size: 32rpx;
    font-weight: 500;
    margin: 24rpx auto 40rpx;
}

.index {
    background-color: #f5f7fc;
    height: 100vh;
}

.index .pre-box .pre-bg {
    margin-top: 40rpx;
}

.index .pre-box .pre-bg > image {
    height: 750rpx;
    width: 750rpx;
}

.index .pre-box .pre-tips {
    color: #000;
    font-size: 32rpx;
    font-weight: 400;
    margin: 80rpx 0rpx 110rpx;
    text-align: center;
}

.index .pre-box .pre-tips .text {
    color: #ff9415;
}

.index .link-box .content {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    flex-direction: column;
    padding: 0rpx 80rpx;
    text-align: left;
}

.index .link-box .content .title {
    color: #000;
    font-size: 56rpx;
    font-style: normal;
    font-weight: 600;
    height: 78rpx;
    line-height: 78rpx;
    margin: 64rpx 0rpx 24rpx;
    position: relative;
}

.index .link-box .content .tips {
    color: #999;
    font-size: 32rpx;
    font-weight: 400;
}

.index .link-box .bg {
    margin: 40rpx 0rpx;
}

.index .link-box .bg image {
    height: 750rpx;
    width: 750rpx;
}

.index .link-box .bottom-tips {
    color: #ff9415;
    font-size: 28rpx;
    text-align: center;
}

.index .link-list-box .title {
    font-size: 56rpx;
    font-weight: 600;
    height: 78rpx;
    line-height: 78rpx;
    margin: 64rpx 0rpx 80rpx 80rpx;
}

.index .link-list-box .list-box {
    padding: 24rpx;
}

.index .link-list-box .list-box .list-item {
    background-color: #fff;
    border-radius: 24rpx;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    height: 176rpx;
}

.index .link-list-box .list-box .list-item > image {
    border-radius: 24rpx 0rpx 0rpx 24rpx;
    height: 176rpx;
    width: 176rpx;
}

.index .link-list-box .list-box .list-item .content {
    flex: 1;
    margin: 32rpx 24rpx;
}

.index .link-list-box .list-box .list-item .content .name {
    color: #000;
    font-size: 28rpx;
    font-weight: 500;
    margin-bottom: 8rpx;
}

.index .link-list-box .list-box .list-item .content .detail {
    color: #999;
    font-size: 24rpx;
}

.index .link-list-box .list-box .list-item .handle {
    align-items: center;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    justify-content: center;
    margin-right: 16rpx;
}

.index .input-wifi-box {
    padding: 0rpx 80rpx;
}

.index .input-wifi-box .title {
    color: #000;
    font-size: 56rpx;
    font-weight: 600;
    margin: 64rpx 0rpx 24rpx;
}

.index .input-wifi-box .tips {
    color: #999;
}

.index .input-wifi-box .input-box {
    margin: 60rpx 0rpx 80rpx;
}

.index .input-wifi-box .input-box .input-item {
    background-color: #fff;
    border-radius: 48rpx;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    padding: 28rpx 40rpx;
}

.index .input-wifi-box .input-box .input-item:first-child {
    margin-bottom: 24rpx;
}

.index .input-wifi-box .input-box .input-item .input {
    border: none;
    color: #000;
    flex: 1;
    font-size: 28rpx;
    margin-right: 24rpx;
    outline: none;
}

.index .input-wifi-box .input-box .input-item .input::-webkit-input-placeholder {
    color: #999;
}

.index .input-wifi-box .input-box .input-item .input::-moz-placeholder {
    color: #999;
}

.index .input-wifi-box .input-box .input-item .input:-ms-input-placeholder {
    color: #999;
}

.index .input-wifi-box .input-box .input-item .input::-ms-input-placeholder {
    color: #999;
}

.index .input-wifi-box .input-box .input-item .input::placeholder {
    color: #999;
}

.index .input-wifi-box .input-box .input-item image {
    height: 40rpx;
    width: 40rpx;
}

.index .search-box {
    padding: 64rpx 80rpx 0rpx;
}

.index .search-box .title {
    color: #000;
    font-size: 56rpx;
    font-weight: 600;
    margin-bottom: 24rpx;
}

.index .search-box .tips {
    color: #999;
    font-size: 32rpx;
}

.index .search-box > image {
    height: 480rpx;
    margin: 40rpx auto 40rpx 54rpx;
    width: 480rpx;
}

.index .search-box .seach-step .step-item {
    align-items: center;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    margin-bottom: 32rpx;
}

.index .search-box .seach-step .step-item.loading image {
    animation: loading 1s infinite;
    height: 40rpx;
    width: 40rpx;
}

.index .search-box .seach-step .step-item image {
    height: 40rpx;
    margin-right: 16rpx;
    width: 40rpx;
}

.index .search-box .seach-step .step-item .step-text {
    color: #999;
    font-size: 28rpx;
}

.index .result-box {
    padding: 40rpx 80rpx 0rpx;
    text-align: center;
}

.index .result-box image {
    height: 240rpx;
    width: 240rpx;
}

.index .result-box image.success {
    margin-top: 152rpx;
}

.index .result-box .title {
    color: #000;
    font-size: 56rpx;
    font-weight: 600;
    margin: 24rpx auto;
}

.index .result-box .tips {
    color: #999;
    font-size: 28rpx;
    margin: 0 auto;
}

.index .result-box .explain {
    color: #666;
    text-align: left;
}

.index .result-box .explain .explain-item {
    font-size: 28rpx;
    line-height: 40rpx;
    text-align: left;
}

.index .result-box .btn {
    margin-top: 108rpx;
}

@-webkit-keyframes loading {
100% {
    transform: rotate(1turn);
}

}
@keyframes loading {
100% {
    transform: rotate(1turn);
}

}
